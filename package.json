    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "typescript": "^3.7.4"
  },
  "keywords": [
    "blog",
    "tech",
    "personal",
    "edouardr"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "npm run prettier -- --write",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|json|css|scss|ts|tsx|md|mdx)\"",
    "serve": "gatsby serve",
    "setup": "npm install && npm run validate",
    "start": "npm run develop",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "validate": "npm-run-all --parallel type-check format lint && npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edouardr/edouardr.fr"
  },
  "bugs": {
    "url": "https://github.com/edouardr/edouardr.fr/issues"
  }
